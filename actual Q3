#include <iostream>
using namespace std;

struct Point {
float x;
float y;
};





int main() {

    Point coordinates[7]; 

for (int i=0; i<7; i++) {
cout<< "Enter the co-ordinate x : "<<endl;
cin >> coordinates[i].x;
cout<< "Enter the co-ordinate y : "<<endl;
cin >> coordinates[i].y;
}


    for (int i = 0; i < 7; i++) {


        if ( coordinates[i].x>0 && coordinates[i].y>0){


            cout<< "the points lying in the first quadrant are "<< "{"<< coordinates[i].x<< " , "<< coordinates[i].y<< "}\n";


        }





    }




    return 0;


}








	
